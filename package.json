{
  "name": "firestorm-hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "emulators": "tsc && vite build -m development && firebase emulators:start",
    "build": "tsc && vite build",
    "buildAll": "cd functions/ && npm run build && cd ../ && tsc && vite build",
    "deploy": "tsc && vite build && firebase deploy --only hosting",
    "deployAll": "npm run buildAll && firebase deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.9",
    "@types/node": "^20.12.4",
    "@uidotdev/usehooks": "^2.4.1",
    "axios": "^1.6.8",
    "react": "^18.2.0",
    "react-custom-scroll": "^7.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-infinite-scroll-hook": "^4.1.1",
    "react-router-dom": "^6.22.3",
    "react-tooltip": "^5.26.3",
    "typescript": "^4.9.5",
    "uuid": "^9.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.11"
  }
}
